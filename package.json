{
  "name": "ncm-analyze-tree",
  "version": "4.0.0",
  "license": "MIT",
  "repository": "nodesource/ncm-analyze-tree",
  "description": "Get certification data for a module's dependency tree",
  "scripts": {
    "test": "standard && npm run test:deps && node example.js",
    "test:deps": "dependency-check . --quiet && dependency-check . --quiet --unused --no-dev",
    "standard": "standard --verbose",
    "standard-fix": "standard --fix"
  },
  "devDependencies": {
    "dependency-check": "^4.1.0",
    "standard": "^16.0.3"
  },
  "dependencies": {
    "graphql-request": "^3.4.0",
    "semver": "^7.3.4",
    "universal-module-tree": "^4.0.0"
  }
}
